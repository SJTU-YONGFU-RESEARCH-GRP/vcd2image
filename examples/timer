:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5807d52d5dc0 .scope module, "tb_timer" "tb_timer" 2 29;
 .timescale -9 -9;
v0x5807d5327760_0 .var "clock", 0 0;
v0x5807d5327800_0 .net "pulse", 0 0, v0x5807d5327510_0;  1 drivers
v0x5807d53278a0_0 .var "reset", 0 0;
E_0x5807d5310e80 .event edge, v0x5807d5327510_0;
E_0x5807d53111f0 .event edge, v0x5807d5327620_0;
S_0x5807d52d5f50 .scope module, "u_timer" "timer" 2 35, 2 3 0, S_0x5807d52d5dc0;
 .timescale -9 -9;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 1 "pulse";
L_0x75938642d018 .functor BUFT 1, C4<1011>, C4<0>, C4<0>, C4<0>;
v0x5807d52fcde0_0 .net/2u *"_ivl_0", 3 0, L_0x75938642d018;  1 drivers
v0x5807d52fd1f0_0 .net "clock", 0 0, v0x5807d5327760_0;  1 drivers
v0x5807d52d7ba0_0 .var "count", 3 0;
v0x5807d5327450_0 .net "count_eq11", 0 0, L_0x5807d5327970;  1 drivers
v0x5807d5327510_0 .var "pulse", 0 0;
v0x5807d5327620_0 .net "reset", 0 0, v0x5807d53278a0_0;  1 drivers
E_0x5807d5311090 .event posedge, v0x5807d52fd1f0_0;
L_0x5807d5327970 .cmp/eq 4, v0x5807d52d7ba0_0, L_0x75938642d018;
    .scope S_0x5807d52d5f50;
T_0 ;
    %wait E_0x5807d5311090;
    %load/vec4 v0x5807d5327620_0;
    %load/vec4 v0x5807d5327450_0;
    %or;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5807d52d7ba0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5807d52d7ba0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5807d52d7ba0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5807d52d5f50;
T_1 ;
    %wait E_0x5807d5311090;
    %load/vec4 v0x5807d5327450_0;
    %assign/vec4 v0x5807d5327510_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5807d52d5dc0;
T_2 ;
T_2.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5807d5327760_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5807d5327760_0, 0, 1;
    %delay 10, 0;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5807d52d5dc0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5807d53278a0_0, 0, 1;
    %pushi/vec4 5, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5807d5311090;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5807d53278a0_0, 0;
    %pushi/vec4 5, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5807d5311090;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5807d53278a0_0, 0;
    %end;
    .thread T_3;
    .scope S_0x5807d52d5dc0;
T_4 ;
T_4.0 ;
    %wait E_0x5807d53111f0;
    %vpi_call 2 59 "$display", "reset=%b at time %0d", v0x5807d53278a0_0, $time {0 0 0};
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x5807d52d5dc0;
T_5 ;
T_5.0 ;
    %wait E_0x5807d5310e80;
    %vpi_call 2 66 "$display", "pulse=%b at time %0d", v0x5807d5327800_0, $time {0 0 0};
    %jmp T_5.0;
    %end;
    .thread T_5;
    .scope S_0x5807d52d5dc0;
T_6 ;
    %delay 1000, 0;
    %vpi_call 2 72 "$display", "Simulation finished." {0 0 0};
    %vpi_call 2 73 "$finish" {0 0 0};
    %end;
    .thread T_6;
    .scope S_0x5807d52d5dc0;
T_7 ;
    %vpi_call 2 77 "$dumpfile", "timer.vcd" {0 0 0};
    %vpi_call 2 78 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5807d52d5dc0 {0 0 0};
    %end;
    .thread T_7;
# The file index is used to find the file name in the following table.
:file_names 3;
